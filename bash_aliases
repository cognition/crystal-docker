# Aliases

alias rebash='source ~/.bashrc '
alias ll='ls -alh' 
alias v='vim '
alias ..='cd ..'

alias histawk='history | awk '

alias l='ls -lh'
alias l.='ls -d .* --color=auto'
alias la='ls -A'
alias ll='ls -alFh'
alias rebash='source ~/.bashrc'
alias v='vim '
alias vdir='vdir --color=auto'
alias vi='vim'


alias x11vnc='x11vnc -forever -usepw -create' 
